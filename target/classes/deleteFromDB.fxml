<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="JAVA_MUSIC.DeleteSongController">
    <children>
        <Label layoutX="166.0" layoutY="86.0" text="Select Song to Delete">
            <font>
                <Font size="24.0" />
            </font>
        </Label>
        <ComboBox fx:id="songComboBox" layoutX="157.0" layoutY="157.0" prefWidth="300.0" />
        <Button fx:id="deleteButton" layoutX="260.0" layoutY="249.0" mnemonicParsing="false" onAction="#deleteSong" text="Delete" />
    </children>
</AnchorPane>